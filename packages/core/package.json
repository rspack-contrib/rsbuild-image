{
  "name": "@rsbuild-image/core",
  "version": "0.0.1-next.6",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "node": "./dist/loader.js",
      "import": "./dist/loader.mjs"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "node": "./dist/shared/index.js",
      "import": "./dist/shared/index.mjs"
    },
    "./types": {
      "types": "./dist/env.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index.d.ts"],
      "loader": ["./dist/loader.d.ts"],
      "shared": ["./dist/shared/index.d.ts"],
      "types": ["./dist/env.d.ts"]
    }
  },
  "sideEffects": ["**/*.css", "dist/logger.*"],
  "files": ["dist"],
  "scripts": {
    "prepare": "pnpm build",
    "build": "rslib build",
    "build:storybook": "storybook build",
    "storybook": "storybook dev --no-open --port 6006",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "VITE_CJS_IGNORE_WARNING=true vitest run",
    "test:watch": "VITE_CJS_IGNORE_WARNING=true vitest",
    "preview-pack": "bash ./scripts/preview-pack.sh"
  },
  "dependencies": {
    "image-size": "^2.0.1",
    "p-memoize": "catalog:",
    "rslog": "catalog:",
    "type-fest": "catalog:",
    "ufo": "catalog:",
    "@sindresorhus/is": "catalog:"
  },
  "devDependencies": {
    "@rsbuild/core": "catalog:",
    "@rsbuild/plugin-react": "catalog:",
    "@rslib/core": "catalog:",
    "@storybook/addon-essentials": "catalog:",
    "@storybook/addon-interactions": "catalog:",
    "@storybook/addon-links": "catalog:",
    "@storybook/addon-onboarding": "catalog:",
    "@storybook/blocks": "catalog:",
    "@storybook/react": "catalog:",
    "@storybook/test": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "jiti": "catalog:",
    "sharp": "catalog:",
    "vitest": "catalog:",
    "ipx": "catalog:",
    "storybook": "catalog:",
    "storybook-addon-rslib": "catalog:",
    "storybook-react-rsbuild": "catalog:"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "sharp": ">=0.33.5",
    "ipx": ">=3.0.3"
  },
  "peerDependenciesMeta": {
    "sharp": {
      "optional": true
    },
    "ipx": {
      "optional": true
    }
  }
}
