{
  "name": "@rsbuild-image/react",
  "version": "0.0.1-next.33",
  "type": "commonjs",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": ["**/*.css", "dist/logger.*"],
  "license": "MIT",
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rspack-contrib/rsbuild-image.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "build": "rslib build",
    "build:storybook": "storybook build",
    "storybook": "storybook dev --no-open --port 6006",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "VITE_CJS_IGNORE_WARNING=true vitest run",
    "test:watch": "VITE_CJS_IGNORE_WARNING=true vitest",
    "preview-pack": "bash ./scripts/preview-pack.sh"
  },
  "dependencies": {
    "debug": "catalog:",
    "type-fest": "catalog:",
    "ufo": "catalog:"
  },
  "devDependencies": {
    "rsbuild-plugin-publint": "catalog:",
    "@rsbuild-image/core": "workspace:*",
    "@rsbuild/core": "catalog:",
    "@rsbuild/plugin-react": "catalog:",
    "@rslib/core": "catalog:",
    "@storybook/addon-essentials": "catalog:",
    "@storybook/addon-interactions": "catalog:",
    "@storybook/addon-links": "catalog:",
    "@storybook/addon-onboarding": "catalog:",
    "@storybook/blocks": "catalog:",
    "@storybook/react": "catalog:",
    "@storybook/test": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/debug": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "ipx": "catalog:",
    "jiti": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "sharp": "catalog:",
    "storybook": "catalog:",
    "storybook-addon-rslib": "catalog:",
    "storybook-react-rsbuild": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@rsbuild-image/core": "*",
    "ipx": ">=3.0.3",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "sharp": ">=0.33.5"
  },
  "peerDependenciesMeta": {
    "sharp": {
      "optional": true
    },
    "ipx": {
      "optional": true
    }
  }
}
