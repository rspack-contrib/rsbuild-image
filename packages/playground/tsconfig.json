{
  "compilerOptions": {
    "lib": ["ES2021", "DOM"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./src",
    "stripInternal": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "types": ["@rsbuild/core/types", "@rsbuild-image/core/types"]
  }
}
